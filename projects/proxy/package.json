{
  "name": "tsh-proxy",
  "version": "1.0.0",
  "description": "ToolVault MCP proxy server",
  "main": "dist/index.js",
  "bin": {
    "tsh": "dist/index.js"
  },
  "scripts": {
    "build": "ncc build src/index.ts -o dist && echo '#!/usr/bin/env node' | cat - dist/index.js > dist/index.tmp && mv dist/index.tmp dist/index.js && chmod +x dist/index.js",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.3",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "toolvault-bridge": "file:../bridge",
    "tsx": "^4.7.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.57",
    "@types/winston": "^2.4.4",
    "@vercel/ncc": "^0.38.3",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
